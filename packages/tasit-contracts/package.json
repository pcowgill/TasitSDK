{
  "name": "tasit-contracts",
  "version": "0.0.12",
  "description": "",
  "main": "dist/index.js",
  "keywords": [
    "ethereum",
    "dapp",
    "truffle",
    "ganache",
    "solidity"
  ],
  "author": "Tasit Labs",
  "homepage": "https://github.com/tasitlabs/TasitSDK/tree/develop/packages/tasit-contracts#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tasitlabs/tasitsdk.git"
  },
  "scripts": {
    "clean": "rm -rf build/ dist/",
    "compile": "npx babel src --out-dir dist --ignore src/*.test.js,src/**/*.test.js",
    "migrate": "npx truffle migrate --reset && npm run populate-contracts",
    "prepare": "npm run install:3rd-parties && npm run babel:compile",
    "test": "npx truffle test",
    "install:3rd-parties": "./3rd-parties/run_on_all.sh install `pwd`",
    "compile:3rd-parties": "./3rd-parties/run_on_all.sh compile `pwd`",
    "populate-contracts": "npx babel-node src/scripts/populateDecentralandContracts.js"
  },
  "bugs": {
    "url": "https://github.com/tasitlabs/tasitsdk/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.1.6",
    "chai-as-promised": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "truffle": "^5.0.8"
  },
  "dependencies": {
    "openzeppelin-solidity": "^2.1.3"
  }
}
